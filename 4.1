3.#include<iostream>
using namespace std;
class Complex
{
	public:
	    Complex(){real=0;imag=0;}
		Complex(int r,int i){real=r;imag=i;}
		Complex operator+(Complex &);
		Complex cinn();
		void display();
	private:
		int real;
		int imag;
};
Complex Complex::operator+(Complex &c2)
{
	Complex c;
	return Complex(real+c2.real,imag+c2.imag);
}
void Complex::display()
{
	cout<<"("<<real<<"+"<<imag<<"i)"<<endl;
}
Complex Complex::cinn()
{
	Complex c4;
	cout<<"请输入一个复数的实部和虚部："<<endl;
	cin>>c4.real>>c4.imag;
	return c4;
}
int main()
{
	Complex c1,c2,c3,c4;
	c1=c1.cinn();c2=c2.cinn();
	cout<<"c1=";c1.display();
	cout<<"c2=";c2.display();
	c3=c1+c2;
	cout<<"c1+c2=";c3.display();
	return 0;
}
